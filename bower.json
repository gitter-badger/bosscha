{
  "name": "kodepot",
  "dependencies": {
    "milligram": "1.0.3",
    "marked": "0.3.5",
    "jsdiff": "2.2.1",
    "sweetalert": "1.1.3",
    "codemirror": "5.11.0",
    "highlightjs": "9.1.0",
    "spin.js": "2.3.2"
  },
  "overrides": {
    "milligram": { "arch": ["client"] },
    "marked": { "arch": ["client", "server"] },
    "sweetalert": { "arch": ["client"] },
    "spin.js": {
      "main": ["spin.js", "jquery.spin.js"],
      "arch": ["client"]
    },
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "addon/mode/overlay.js",
        "mode/xml/xml.js",
        "mode/markdown/markdown.js",
        "mode/gfm/gfm.js",
        "mode/javascript/javascript.js",
        "mode/css/css.js",
        "mode/htmlmixed/htmlmixed.js",
        "mode/clike/clike.js",
        "mode/ruby/ruby.js",
        "mode/go/go.js",
        "lib/codemirror.css",
        "theme/paraiso-dark.css"
      ],
      "arch": ["client"]
    },
    "highlightjs": {
      "main": ["highlight.pack.js", "styles/github.css"],
      "arch": ["client"]
    }
  },
  "private": true
}
