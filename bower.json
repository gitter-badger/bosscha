{
  "name": "kodepot",
  "dependencies": {
    "marked": "0.3.5",
    "codemirror": "5.11.0",
    "highlightjs": "9.1.0",
    "sweetalert": "1.1.3",
    "spinkit": "1.2.5",
    "jsdiff": "2.2.1"
  },
  "overrides": {
    "marked": { "arch": ["client", "server"] },
    "sweetalert": { "arch": ["client"] },
    "spinkit": { "arch": ["client"] },
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "addon/fold/foldcode.js",
        "addon/fold/foldgutter.js",
        "addon/fold/brace-fold.js",
        "addon/fold/xml-fold.js",
        "addon/fold/markdown-fold.js",
        "addon/fold/comment-fold.js",
        "addon/mode/overlay.js",
	"addon/display/placeholder.js",
        "mode/xml/xml.js",
        "mode/markdown/markdown.js",
        "mode/gfm/gfm.js",
        "mode/javascript/javascript.js",
        "mode/css/css.js",
        "mode/htmlmixed/htmlmixed.js",
        "mode/clike/clike.js",
        "mode/ruby/ruby.js",
        "mode/go/go.js",
        "lib/codemirror.css",
	"addon/fold/foldgutter.css",
        "theme/elegant.css"
      ],
      "arch": ["client"]
    },
    "highlightjs": {
      "main": ["highlight.pack.js", "styles/github.css"],
      "arch": ["client"]
    }
  },
  "private": true
}
